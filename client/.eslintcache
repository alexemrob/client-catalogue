;\n\nclass Results extends Component {\n    state = {\n        savedClients: [],\n    }\n\n    componentDidMount() {\n        API.savedClients()\n            .then(savedClients => this.setState({ savedClients: savedClients }))\n            .catch(err => console.error(err));\n    }\n\n    handleSave = client => {\n        if (this.state.savedClients.map(client => client._id).includes(client._id)) {\n            API.deleteClient(client._id)\n                .then(deletedClient => this.setState({ savedClients: this.state.savedClients.filter(client => client._id !== deletedClient._id) }))\n                .catch(err => console.error(err));\n        } else {\n            API.addClient(client)\n                .then(savedClient => this.setState({ savedClients: this.state.savedClients.concat([savedClient]) }))\n                .catch(err => console.error(err));\n        }\n    }\n\n\n    render() {\n        return (\n            <div className=\"results-card d-flex justify-content-start\">\n                {!this.props.clients.length ? (\n                    <h4 className=\"text-center\">No Results to Display</h4>\n                ) : (\n                        <div>\n                            {this.props.clients.map(result => (\n                                <div className=\"box card mb-3\" >\n                                    <div className=\"row glass\">\n                                        <div className=\"card-body col-md-10\">\n                                            <h2 className=\"card-title\" key={result._id}>{result.name}</h2>\n                                            <p><b>Phone Number:</b> {result.phone}</p>\n                                            <p><b>Email Address: </b>{result.email}</p>\n                                            <p><b>Referal: </b>{result.referBy}</p>\n                                            <p className=\"card-text\"><b>Formula + Notes: </b>{result.notes}</p>\n                                            <div className=\"d-flex justify-content-end\">\n                                                <Link className=\"btn btn-outline-dark mt-3\" to={`/edit/${result._id}`}>Edit</Link>\n                                                <button onClick={() => this.handleSave(result)} className=\"btn btn-outline-dark mt-3 ml-3\" >\n                                                    {this.state.savedClients.map(client => client._id).includes(result._id) ? \"- Today\" : \"+ Today\"}\n                                                </button>\n                                                <a className=\"btn btn-outline-dark mt-3\" target=\"_blank\" href={`www.instagram.com/${result.insta}`}><FaInstagram /></a>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            ))}\n                        </div>\n                    )}\n            </div>\n        )\n    }\n}\n\nexport default Results;\n","C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\NavBar\\index.js",["98"],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Checkout\\index.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Notes\\TodoList.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Footer\\index.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Notes\\TodoForm.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Notes\\Todo.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Search\\index.js",["99"],"import React from \"react\";\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport './style.css'\n\nfunction Form(props){\n  return (\n  <div className=\"container\">\n  <form>\n    <div className=\"form-group1 d-flex justify-content-center\">\n      <input\n        onChange={props.handleInputChange}\n        value={props.search}\n        name=\"search\"\n        type=\"text\"\n        className=\"form-control search-bar\"\n        placeholder=\"Search for a Client\"\n        id=\"search\"\n      />\n      <a onClick={props.handleFormSubmit} className=\"btn-neon\">\n        Search\n        </a>\n    </div>\n  </form>\n  </div>\n);\n}\nexport default Form;","C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\pages\\NewClient.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\ClientForm\\index.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\pages\\Edit.js",["100"],"import React, { useState, useEffect } from \"react\";\r\nimport API from \"../utils/API\";\r\nimport { useRouteMatch, useHistory } from \"react-router-dom\";\r\nimport { ClientForm } from \"../components/ClientForm\";\r\nimport { toast } from \"react-toastify\";\r\n\r\nexport const Edit = () => {\r\n  const match = useRouteMatch()\r\n  const [client, setClient] = useState();\r\n  const history = useHistory()\r\n\r\n  useEffect(() => {\r\n    const fetchClient = async () => {\r\n      const client = await API.findClient(match.params.id)\r\n      setClient(client.data)\r\n    }\r\n    fetchClient()\r\n  }, []);\r\n\r\n  const onSubmit = async (data) => {\r\n    await API.updateEdit(data, match.params.id)\r\n    toast(`${data.name}'s information was successfully updated!`, { type: \"success\" });\r\n    history.push(\"/\")\r\n  }\r\n\r\n\r\n  return client ? ( \r\n    <div className=\"container\">\r\n      <div className=\"mt-3\">\r\n        <h3>Edit Client:</h3>\r\n        <ClientForm client={client} onSubmit={onSubmit}/>\r\n      </div>\r\n    </div>\r\n  ) : (\r\n    <div>Loading...</div>\r\n  );\r\n};\r\n\r\nexport default Edit","C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\pages\\Message.js",[],"C:\\Users\\alexe\\Project3\\client-catalogue\\client\\src\\components\\Navbar\\index.js",["101"],{"ruleId":"102","replacedBy"